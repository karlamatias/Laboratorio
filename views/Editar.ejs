<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/update/<%= data.usuario %>" method="POST">
          <div class="form-group">
            <input type="text" name="usuario" value="<%= data.usuario%>" placeholder="Usuario" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="password" value="<%= data.password%>" placeholder="Password" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="rol" value="<%= data.rol%>" placeholder="Rol" class="form-control">
          </div>

          <button type="submit" class="btn btn-primary">
            Actualizar
          </button>
        </form>
      </div>
    </div>
  </div>
</div>
